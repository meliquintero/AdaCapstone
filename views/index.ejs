<div class="row">
  <% searchresults.destinations.forEach(function(destination) { %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="" alt="...">
      <div class="caption">

        <h3><a href="destinations/<%=searchresults['originOne']['airportCode']%>/<%=searchresults['originTwo']['airportCode']%>/<%= destination.airportCode %>/<%=depdate%>/<%=retdate%>"><%= destination.city %> </a></h3>
        <h4><%= destination.country %></h4>
        <p>
          From: <%=searchresults['originOne']['airportName']%> $$ <%= destination.priceFromOriginOne %>
          <br>
          From: <%=searchresults['originTwo']['airportName']%> $$ <%= destination.priceFromOriginTwo  %>
          <br>
          <br>
          <strong> TOTAL:  <%= destination.sumPrices %></strong>
          <p> <a href="destinations/<%=searchresults['originOne']['airportCode']%>/<%=searchresults['originTwo']['airportCode']%>/<%= destination.airportCode %>/<%=depdate%>/<%=retdate%>" class="btn btn-default" role="button">See more flights to this city</a></p>
        </p>
      </div>
    </div>
  </div>
  <% }); %>
</div>
